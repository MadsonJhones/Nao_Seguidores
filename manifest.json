{
    "manifest_version": 3,
    "name": "Gestor Pro",
    "version": "1.1.0",
    "description": "Gerencia contas que n√£o te seguem de volta no Instagram.",
    "permissions": [
        "storage",
        "scripting",
        "tabs",
        "cookies"
    ],
    "host_permissions": [
        "https://*.instagram.com/*",
        "*://*.fbcdn.net/*",
        "https://*.cdninstagram.com/*"
    ],
    "background": {
        "service_worker": "background.js"
    },
    "action": {
        "default_icon": {
            "16": "images/icon16.png",
            "48": "images/icon48.png",
            "128": "images/icon128.png"
        },
        "default_title": "Abrir Gestor Pro"
    },
    "icons": {
        "16": "images/icon16.png",
        "48": "images/icon48.png",
        "128": "images/icon128.png"
    }
}